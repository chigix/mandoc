{
  "name": "mandoc-repository",
  "version": "1.1.2",
  "description": "Yet another MD document converter.",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "echo \"Error: do 'publish.sh' instead of 'npm publish'\" && exit 1",
    "commit": "git-cz",
    "release": "standard-version",
    "mandoc": "ts-node  --project tsconfig/tsconfig.base.json ./src/bin/mandoc.ts",
    "mandoc-gen": "ts-node  --project tsconfig/tsconfig.base.json ./src/bin/mandoc-gen.ts",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "npm-run-all build-clean ts-build build-package build-tpls",
    "build-clean": "shx rm -rf ./dist",
    "build-package": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./build/make-packages.ts",
    "build-source-map-clean": "ts-node ./build/make-clean-source-map.ts",
    "build-tpls": "ts-node ./build/make-templates.ts",
    "ts-build": "tsc -p tsconfig/tsconfig.base.json && tsc -p tsconfig/tsconfig.build.json",
    "ts-lint": "tslint -c tslint.json --project tsconfig/tsconfig.base.json \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chigix/mandoc.git"
  },
  "keywords": [
    "markdown",
    "pdf",
    "convert",
    "template",
    "pandoc",
    "mandoc"
  ],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "author": "Richard Lea <chigix@zoho.com>",
  "homepage": "https://github.com/chigix/mandoc",
  "bugs": {
    "url": "https://github.com/chigix/mandoc/issues"
  },
  "private": true,
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.18.0",
    "fs-extra": "^7.0.0",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "jsonlint": "^1.6.3",
    "keiyaku-css": "^1.1.0",
    "less": "^3.8.1",
    "lodash": "^4.17.10",
    "mathjax": "^2.7.5",
    "mathjs": "^5.2.1",
    "normalize.css": "^8.0.0",
    "nunjucks": "^3.1.3",
    "puppeteer": "^1.9.0",
    "remarkable": "^1.7.1",
    "requireg": "^0.2.1",
    "rxjs": "^6.2.2",
    "slash": "^2.0.0",
    "through2": "^3.0.0",
    "tmp": "0.0.33",
    "uniqid": "^5.0.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^23.3.5",
    "@types/jquery": "^3.3.5",
    "@types/less": "^3.0.0",
    "@types/lodash": "^4.14.113",
    "@types/mathjs": "^4.4.1",
    "@types/node": "^8.9.5",
    "@types/nunjucks": "^3.1.0",
    "@types/puppeteer": "^1.9.0",
    "@types/remarkable": "^1.7.0",
    "@types/shelljs": "^0.8.0",
    "@types/through2": "^2.0.33",
    "@types/tmp": "0.0.33",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.4.2",
    "jest-config": "^23.4.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.2",
    "shx": "^0.3.2",
    "standard-version": "^4.4.0",
    "string-length": "^2.0.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "tslint-no-circular-imports": "^0.6.1",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
